<body>
    <%- include('includes/navigation.ejs') %>


    <main>
        <h1>My Posts</h1>
        <% if (pos.length > 0) { %>
            <% for (let post of pos) {%>
            <div class="card">
                <img src="http://www.nretnil.com/avatar/LawrenceEzekielAmos.png" alt="Avatar">
                <div class="container">
                    <p><%= post.title %></p>
                </div>
            </div>
            <% } %>
        <% } else { %>
        <h1> No Posts</h1>
        <% } %>
    </main>

    <%- include('includes/end.ejs') %>